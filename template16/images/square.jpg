<script>

    //新增
    $("header .mui-pull-left").click(function(){
        $(".mui-off-canvas-backdrop").css("display", "block");
        $("#offCanvasSide").css("left", "0px");

    })
    $(".mui-off-canvas-backdrop").click(function(){
        $(".mui-off-canvas-backdrop").css("display", "none");
        $("#offCanvasSide").css("left", "-100%");
    })

    $('#offCanvasSide').on('click','#newdetail', function(){
        layer.open({
            type: 1,
            title: false,
            closeBtn: 0,
            area:  ['90%', '300px'],
            skin: 'layui-layer-nobg', //没有背景色
            shadeClose: true,
            btn: '关闭',
            content: $('#picture')
        });

    });

    TouchSlide({
        slideCell: "#slideBox",
        titCell: ".hd ul", //开启自动分页 autoPage:true ，此时设置 titCell 为导航元素包裹层
        mainCell: ".bd ul",
        effect: "leftLoop",
        autoPage: true,//自动分页
        autoPlay: true //自动播放
    });

    function adsorption() {
        var headerWrap = document.getElementById('header-wrap');
        var scrollTop = 0;
        window.onscroll = function () {
            scrollTop = document.body.scrollTop || document.documentElement.scrollTop;
            if(headerWrap){
	            if (scrollTop > 1500) {
	                headerWrap.className = 'fixed';
	            } else {
	                headerWrap.className = 'header-wrap';
	            }
            }
        }
    }
    adsorption();

    $("body").eq(0).on('click','.adv span', function(){
    	$(this).parent(".adv").css("display", "none");
    });
</script>
<script type="text/javascript">
	$(document).ready(function(){
		$('#searchmsg').bind('keyup', function(event) {
			if (event.keyCode == "13") {
				tosearch();
			}
		});
		articlelistshow(0,10,'zxlist',null);
	});
	function tosearch(flag){
		if(flag){
			window.open("http://www.ncd.org.cn/search.html");
		}
		if($("#searchmsg").val()){
			window.open("http://www.ncd.org.cn/search.html?s="+$('#searchmsg').val());
		}
	}
	
</script>

